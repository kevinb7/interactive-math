.popup-container
  display inline-block
  text-align center

  ul.menu-container
    background-color rgba(64,64,64,0.75)
    padding 1px
    text-align left
    border-radius 8px

    margin 5px 0
    list-style-type none

    li
      color #ffffff
      font-family sans-serif
      font-size 18px
      font-weight 100
      margin 10px

    li:hover
      color yellow
      cursor pointer
